/**
 * Description of TransferObject
 *
 * @author Lucas
 */
class TransferObject implements JsonSerializable{
    
    private $attrbiutes = array();
    
    public function __construct() {
        
    }
    
    public function __get($name) {
        
        if(array_key_exists($name,$this->attrbiutes))
            return $this->attrbiutes[$name];
       return null;
    }
    
    public function __set($name, $value) {
        
        $this->attrbiutes[$name] = $value;
    }
    
   public function getAttr()
   {
       return $this->attrbiutes;
   }
   
   public function setAttr($a)
   {
       $this->attrbiutes = $a;
   }
   
   public function jsonSerialize()
    {
        return $this->getAttr();
    }
}
